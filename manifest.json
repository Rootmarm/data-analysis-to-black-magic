{
	"manifest_version": 2,

	"name": "Data Analysis to Black Magic",
    "short_name": "Black Magic",
	"description": "Replaces the text 'data analysis' with 'black magic'",
	"author": "Rootmarm",
	"version": "2.1",

	"icons": {
		"16": "icon16.png",
		"32": "icon32.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},

	"content_scripts":
	[
		{
			"matches": ["*://*/*"],
			"all_frames": true,
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	]
}
